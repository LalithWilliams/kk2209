#include<stdio.h>
int main ()
{
  int a,b, choice;
  printf("Enter two integer values: ");
  scanf("%d %d",&a,&b);
  printf("\nEnter 1 to find sum of both the given integer values");
  printf("\nEnter 2 to find the product of both the given integer values\n");
  scanf("%d",&choice);
  if (choice == 1)
  printf("The sum is: %d",a+b);
  else if (choice == 2)
  printf("The product is: %d",a*b);
  else printf("invalid");
  return 0;
  }
